  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json --ignore-watch .next server/vjcSystem.ts",
    "dev:next": "next dev",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "cross-env NODE_ENV=production ts-node --project tsconfig.server.json server/vjcSystem.ts",
    "start2": "cross-env NODE_ENV=production node dist/server/vjcSystem.js",
    "start:next": "next start",
    "lint": "eslint --fix",
    "type-check": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "pm2-next": "pm2 yarn run start",
    "pm2-next-cluster": "next build && pm2 start ./node_modules/next/dist/bin/next-start -i max"
  },
